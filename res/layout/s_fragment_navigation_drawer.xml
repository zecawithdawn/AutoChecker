    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true">

        <ImageView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="14dp"
            android:scaleType="fitStart"
            android:src="@drawable/menu" />


        <ImageView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/img"
            android:layout_width="105dp"
            android:layout_height="105dp"
            android:layout_below="@id/back"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="13dp"
            android:layout_marginTop="5dp"
            android:scaleType="fitCenter"
            android:src="@drawable/shin" />

        <TextView
            android:id="@+id/s_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/img"
            android:layout_marginBottom="5dp"
            android:fontFamily="sans-serif-light"
            android:text="강채영"
            android:textAlignment="center"
            android:textColor="#8EBE3B"
            android:textSize="22dp"

            />

        <TextView
            android:id="@+id/s_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/s_name"
            android:layout_marginBottom="5dp"
            android:fontFamily="sans-serif-light"
            android:text="201300622774"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/s_mail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/s_id"
            android:layout_marginBottom="25dp"
            android:layout_marginTop="3dp"
            android:drawableBottom="@drawable/shape"
            android:fontFamily="sans-serif-light"
            android:paddingBottom="10dp"
            android:text="hannah0409@naver.com"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="13dp" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/s_mail"
            android:layerType="software"
            android:src="@drawable/shape" />

        <ListView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/gg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/s_mail"
            android:choiceMode="singleChoice"
            android:divider="@drawable/shape"
            android:dividerHeight="2dp"
            android:layerType="software"


            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_above="@+id/logout"
            android:layerType="software"
            android:src="@drawable/shape" />

        <ImageView
            android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/gg"
            android:layout_marginRight="10dp"
            android:layout_marginTop="38dp"
            android:src="@drawable/logoutimg"

            />

    </RelativeLayout>
</RelativeLayout>

